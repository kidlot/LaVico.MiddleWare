<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
 
<web-app>

   <display-name>WeLab</display-name>
   
   <servlet>
      <servlet-name>HelloWorld</servlet-name>
      <servlet-class>com.welab.lavico.middleware.HelloWorld</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>HelloWorld</servlet-name>
      <url-pattern>/HelloWorld</url-pattern>
   </servlet-mapping>


   <servlet>
      <servlet-name>MemberApply</servlet-name>
      <servlet-class>com.welab.lavico.middleware.rest.MemberApply</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>MemberApply</servlet-name>
      <url-pattern>/MemberApply</url-pattern>
   </servlet-mapping>

   <servlet>
      <servlet-name>MemberUnbind</servlet-name>
      <servlet-class>com.welab.lavico.middleware.rest.MemberUnbind</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>MemberUnbind</servlet-name>
      <url-pattern>/MemberUnbind</url-pattern>
   </servlet-mapping>

   <servlet>
      <servlet-name>MemberBind</servlet-name>
      <servlet-class>com.welab.lavico.middleware.rest.MemberBind</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>MemberBind</servlet-name>
      <url-pattern>/MemberBind</url-pattern>
   </servlet-mapping>

   <servlet>
      <servlet-name>Promotions</servlet-name>
      <servlet-class>com.welab.lavico.middleware.rest.Promotions</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>Promotions</servlet-name>
      <url-pattern>/Promotions</url-pattern>
   </servlet-mapping>

   <servlet>
      <servlet-name>Points</servlet-name>
      <servlet-class>com.welab.lavico.middleware.rest.Points</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>Points</servlet-name>
      <url-pattern>/Points</url-pattern>
   </servlet-mapping>
   
</web-app>